# Keyway Environment Variables
# Copy this file to .env.local and update the values

# Signaling Server URL (required for production)
# Set this to your deployed signaling server URL
# Example: https://keyway-signaling.railway.app
NEXT_PUBLIC_SIGNALING_URL=http://localhost:3001

# Signaling Server Port (for the signaling server itself)
SIGNALING_PORT=3001
